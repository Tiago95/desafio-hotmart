{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "desafio-hotmart"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "ms",
      "styles": [
        "assets/scss/style.scss",
        "assets/fonts/data-table/icons.css",
        "assets/linea/styles.css",
        "assets/glyphicons/glyphicons.css",
        "assets/climacon/climacon-font/styles.css",
        "assets/climacon/climacon.css",
        "../node_modules/quill/dist/quill.snow.css",
        "../node_modules/leaflet/dist/leaflet.css",
        "../node_modules/angular-calendar/dist/css/angular-calendar.css",
        "../node_modules/@swimlane/ngx-datatable/release/index.css",
        "../node_modules/@swimlane/ngx-datatable/release/themes/material.css",
        "../node_modules/font-awesome/scss/font-awesome.scss",
        "../node_modules/simple-line-icons/scss/simple-line-icons.scss",
        "../node_modules/dragula/dist/dragula.css",
        "../node_modules/perfect-scrollbar/css/perfect-scrollbar.css",
        "assets/cropper/cropper.css"

      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/tether/dist/js/tether.min.js",
        "../node_modules/hammerjs/hammer.js",
        "../node_modules/sortablejs/Sortable.js",
        "../node_modules/ace-builds/src-min/ace.js",
        "../node_modules/ace-builds/src-min/theme-eclipse.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "assets/cropper/cropper.js"
      ],
      "environmentSource": "environments/environment.ts",
        "environments": {
          "dev": "environments/environment.ts",
          "prod": "environments/environment.prod.ts"
        }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
